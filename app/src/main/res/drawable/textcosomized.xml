<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

        <item
            android:bottom="1dp"
            android:left="5dp"
            android:right="5dp"
            android:top="5dp">
                <shape android:shape="rectangle">
                        <stroke
                            android:width="0.5dp"
                            android:color="@android:color/black" />
                        <solid android:color="#FCFBFB" />
                        <stroke
                            android:width="3dp"
                            android:color="#000000"
                            android:dashGap="0dp"
                            android:dashWidth="5dp" />
                        <padding
                            android:bottom="5dp"
                            android:left="5dp"
                            android:right="5dp"
                            android:top="5dp" />
                        <corners
                            android:radius="10dp"/>
                </shape>
        </item>
</layer-list>

